{
  "manifest_version": 2,

  "name": "GistChrome",
  "description": "This extension allow user to create Github Gist",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon2.png",
    "default_popup": "gistchrome.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ]
}